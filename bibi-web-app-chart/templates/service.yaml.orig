apiVersion: v1
kind: Service
metadata:
  name: {{ include "bibi-web-app-chart.fullname" . }}
  labels:
    {{- include "bibi-web-app-chart.labels" . | nindent 4 }}
spec:
  type: {{ .Values.service.type }}
  ports:
    - port: {{ .Values.service.port }}
      targetPort: http
      protocol: TCP
      name: http
  selector:
    {{- include "bibi-web-app-chart.selectorLabels" . | nindent 4 }}
